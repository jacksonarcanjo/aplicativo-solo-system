# .env.example

# Gemini API Key (Required for AI features)
GEMINI_API_KEY=

# VAPID Keys for Web Push Notifications (Optional)
# Generate using: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# Twilio Configuration for WhatsApp (Optional - Required for real WhatsApp sending)
# Get these from your Twilio Console: https://www.twilio.com/console
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=
